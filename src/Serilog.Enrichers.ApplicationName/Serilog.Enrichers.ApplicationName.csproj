<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Enrich Serilog log events with the application name.</Description>
    <TargetFramework>netstandard1.3</TargetFramework>
    <Authors>engelbert strauss GmbH &amp; Co. KG</Authors>
    <PackageId>Serilog.Enrichers.ApplicationName</PackageId>
    <PackageTags>serilog;applicationname;enricher</PackageTags>
    <PackageIconUrl>http://serilog.net/images/serilog-enricher-nuget.png</PackageIconUrl>
    <PackageProjectUrl>http://serilog.net</PackageProjectUrl>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.PlatformAbstractions" Version="1.1.0" />
    <PackageReference Include="Serilog" Version="2.6.0" />
  </ItemGroup>

</Project>
